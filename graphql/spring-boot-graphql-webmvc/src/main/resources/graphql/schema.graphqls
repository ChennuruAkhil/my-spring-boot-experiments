type Author{
    id:ID!
    name:String!
    email:String!
    version:Int !
    posts: [Post]
}

type Post{
    id:ID!
    title:String!
    content:String!
    createdOn:String
    details: PostDetails!
    comments: PostComments
}

type PostComments {
    id:ID
    review:String
}

type PostDetails {
    id:ID!
    createdOn:String
}

type Query{
     allAuthors: [Author!]!
}
