type Query {
    getPostsByUserName(name : String) : [PostsDTO!]
}

type Mutation {
    createPostByUserName(name : String) : [PostRequestDTO]
}

type PostRequestDTO {
    title: String
    content: String
    comments: PostCommentsDTO!
    tags: TagDTO!
}

type TagDTO {
    name: String
}

type PostsDTO {
    title: String
    content: String
    createdBy: String
    createdOn: String
    comments: PostCommentsDTO!
}

type PostCommentsDTO {
    review: String
}